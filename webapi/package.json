{
  "name": "webapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon -r dotenv/config ./bin/www",
    "start": "node ./bin/www",
    "lint": "eslint .",
    "test": "cross-env DOTENV_CONFIG_PATH=\"./.env.test\" mocha -r dotenv/config --exit",
    "db:migrate": "node -r dotenv/config scripts/migrate",
    "make:migration": "knex migrate:make",
    "db:rollback": "node -r dotenv/config scripts/rollback",
    "db:seed": "node -r dotenv/config database/seeds"
  },
  "dependencies": {
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "knex": "^0.21.1",
    "lodash": "^4.17.20",
    "morgan": "~1.9.1",
    "mssql": "^6.2.0",
    "multer": "^1.4.2",
    "objection": "^2.1.3",
    "objection-guid": "^3.0.2",
    "objection-unique": "^1.2.0",
    "objection-visibility": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3"
  }
}
